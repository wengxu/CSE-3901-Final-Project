<div id = "ulist">
<h1>Search Results</h1>

<table>
  <thead>
    <tr>
      <th>Group Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @groups.each do |group| %>
      <tr>
        <td><%= group.name %></td>
	   <td><%= link_to 'Join', join_group_path(group) %></td>
        <td><%= link_to 'Show', group %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>



</div>
